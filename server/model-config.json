{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "sql-azure"
  },
  "AccessToken": {
    "dataSource": "sql-azure",
    "public": false
  },
  "ACL": {
    "dataSource": "sql-azure",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "sql-azure",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "sql-azure",
    "public": false
  },
  "Product": {
    "dataSource": "sql-azure",
    "public": true
  },
  "Brand": {
    "dataSource": "sql-azure",
    "public": true
  },
  "Dealer": {
    "dataSource": "sql-azure",
    "public": true
  },
  "Order": {
    "dataSource": "sql-azure",
    "public": true
  },
  "OrderDetail": {
    "dataSource": "sql-azure",
    "public": true
  },
  "OrderDetailIMEI": {
    "dataSource": "sql-local",
    "public": true
  },
  "OrderTrack": {
    "dataSource": "sql-local",
    "public": true
  },
  "OrderPayment": {
    "dataSource": "sql-local",
    "public": true
  },
  "ProductDealer": {
    "dataSource": "sql-azure",
    "public": true
  },
  "Kiosk": {
    "dataSource": "sql-azure",
    "public": true
  },
  "KioskProductDealer": {
    "dataSource": "sql-local",
    "public": true
  },
  "DealerUser": {
    "dataSource": "sql-local",
    "public": true
  },
  "Configuration": {
    "dataSource": "sql-local",
    "public": true
  },
  "VMappedProduct": {
    "dataSource": "sql-local",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findById": true,
          "findOne": true,
          "count": true,
          "exists": true
        }
      }
    }
  }
}
